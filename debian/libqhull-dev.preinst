#!/bin/sh
# preinst script for libqhull-dev

set -e

case "$1" in
    install|upgrade)
	if test -L /usr/share/doc/libqhull-dev ; then
		rm -rf /usr/share/doc/libqhull-dev
        fi

    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0


